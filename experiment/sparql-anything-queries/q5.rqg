PREFIX source: <urn:facade-x:ns#>

SELECT DISTINCT ?id
WHERE{

	SERVICE <x-sparql-anything:data/COLLEZIONI_PALAZZO_MADAMA_marzo2017.json> {
		?s ?p ?subject .
		?s source:Inventario ?id .
		FILTER(REGEX(?subject,".*manifattura Hochst.*","i"))
		FILTER(str(?p) = "urn:facade-x:ns#Ambito culturale")
	}
}
